##########################
# For pagodabox 
# You can ignore this
##########################
url_rewrites:
    -
        rule: "^/(.+)$ index.php?i=$1"
        conditions:
            - "!-f $request_filename"
            - "!-d $request_filename"
php_extensions: [mysql, curl]
php_error_reporting: "E_ALL & ~E_NOTICE"
